{
  "name": "@yehonathan-bar-ilan/find-parents",
  "version": "1.0.2",
  "description": "A tool to find the direct parents of a specific npm dependency",
  "main": "find-parents.mjs",
  "type": "module",
  "bin": {
    "find-parents": "find-parents.mjs"
  },
  "scripts": {
    "start": "node find-parents.mjs",
    "test": "jest",
    "lint": "eslint .",
    "stryker": "stryker run"
  },
  "dependencies": {
    "@yehonathan-bar-ilan/file-ops": "^1.0.4",
    "axios": "^1.7.9",
    "commander": "^8.0.0",
    "globals": "^15.14.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/eslintrc": "^3.2.0",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/mocha-runner": "^8.7.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "stryker": "^1.0.1",
    "stryker-api": "^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YehonathanBarIlan/find-parents.git"
  },
  "homepage": "https://github.com/YehonathanBarIlan/find-parents#readme",
  "bugs": {
    "url": "https://github.com/YehonathanBarIlan/find-parents/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.(js|mjs)",
    "collectCoverageFrom": [
      "**/*.{js,jsx,mjs}"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "roots": [
      "./domain/",
      "./"
    ]
  }
}
